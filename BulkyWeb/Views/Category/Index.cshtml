@model List<Category>

<!DOCTYPE html>

<html lang="en">
<head>
    <title>Category List</title>
</head>
<body>
<div>
    <h1>Category List</h1>
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Category Name</th>
                <th>Display Order</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var item in Model.OrderByDescending(u => u.DisplayOrder))
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.DisplayOrder</td>
            </tr>
        }
        </tbody>
    </table>
</div>
</body>
</html>